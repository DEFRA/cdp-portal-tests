{
  "env": {
    "es2022": true,
    "node": true,
    "jest": true
  },
  "globals": {
    "before": true
  },
  "extends": [
    "standard",
    "prettier",
    "eslint:recommended",
    "plugin:wdio/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["prettier", "wdio"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "error"
  }
}
